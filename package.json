    "axios-mock-adapter": "^2.1.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "coveralls": "^3.1.1",
    "jest": "^29.7.0",
    "nyc": "^17.1.0",
    "ts-jest": "^29.3.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.2"
